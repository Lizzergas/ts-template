{
  "name": "js-scratch",
  "version": "1.0.0",
  "description": "learning js",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "NODE_EXTRA_CA_CERTS=./ca.pem tsx index.ts",
    "start:unsafe": "NODE_TLS_REJECT_UNAUTHORIZED=0 tsx index.ts",
    "check": "npm run check:types && npm run check:lint",
    "types": "tsc --noEmit",
    "lint": "biome check .",
    "fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "test"
  },
  "author": "lizz",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.1",
    "https-proxy-agent": "^7.0.6",
    "openai": "^5.10.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/node": "^24.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
